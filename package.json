{
    "name": "contracting-accounting-tool",
    "version": "1.0.0",
    "description": "Custom accounting tool for incorporated contracting business",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
        "backend": "cd backend && go run main.go",
        "frontend": "cd frontend && npm run dev",
